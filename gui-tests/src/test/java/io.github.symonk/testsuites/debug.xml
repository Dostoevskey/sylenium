<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Debug" verbose="2" parallel="methods">
    <listeners>
        <listener class-name="io.github.symonk.listeners.TestExecutionListener"/>
        <listener class-name="io.github.symonk.listeners.AnnotationTransformationListener"/>
    </listeners>
    <test name="Retrying test">
        <classes>
            <class name="io.github.symonk.testcases.RetryTests">
                <methods>
                    <include name="retriedOnFailure"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>
