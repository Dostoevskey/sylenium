node {
   def mvnHome
   stage('FETCH SRC') {
     git 'https://github.com/symonk/Selenide-TestNG-Allure2-Framework.git'
   }
   catchError {
   stage('Execution') {
     bat '"gui-tests/regression_jenkins.bat"'
   }
   }
   stage('REPORT') {
     allure includeProperties: false, jdk: '', results: [[path: 'gui-tests/target/allure-results']]
    }
   stage('LOGGING') {
    archiveArtifacts 'gui-tests/target/test_logs/*.json'
   }
 }